{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": [
        "./src/app/core/*"
      ],
      "@env/*": [
        "./src/environment/*"
      ],
      "@routes": [
        "./src/app/app.routes.ts"
      ],
      "@layout": [
        "./src/app/layout"
      ],
      "@remote/*": [
        "./src/app/remote/*"
      ],
      "@ycyw/shared": [
        "../../dist/shared"
      ],
      "@themes": [
        "../../themes"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
