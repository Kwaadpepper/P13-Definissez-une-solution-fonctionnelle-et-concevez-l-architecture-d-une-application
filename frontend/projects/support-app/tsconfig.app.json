{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@support-core/*": [
        "./src/app/core/*"
      ],
      "@support-domains/*": [
        "./src/app/domains/*"
      ],
      "@support-shard/*": [
        "./src/app/shard/*"
      ],
      "@ycyw/shared": [
        "../../dist/shared"
      ],
      "@themes": [
        "../../themes"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
