{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@ycyw/shared": [
        "../../dist/shared"
      ],
      "@themes": [
        "../../themes"
      ],
      "core/*": [
        "./src/app/core/*"
      ],
      "domains/*": [
        "./src/app/domains/*"
      ],
      "shared/*": [
        "./src/app/shared/*"
      ],
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
